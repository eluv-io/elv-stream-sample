{
  "name": "elv-stream-sample",
  "version": "3.0.26",
  "description": "",
  "main": "index.js",
  "license": "MIT",
  "homepage": "https://github.com/eluv-io/elv-stream-sample",
  "scripts": {
    "bump-version": "npm --git-tag-version --no-commit-hooks version patch",
    "serve": "webpack-dev-server --hot --port 8084",
    "serve-display-app": "webpack-dev-server --entry ./src/pages/DisplayApp.js --hot --port 8084",
    "build": "rm -rf dist/* && NODE_ENV=production node ./node_modules/webpack-cli/bin/cli.js --output-path ./dist/ -p --progress --colors --devtool none",
    "add-build": "git add dist",
    "lint": "npm run fix-scss ; npm run lint-scss ; npm run fix-js",
    "lint-show": "npm run lint-scss ; npm run lint-js",
    "lint-js": "./node_modules/.bin/eslint \"src/**/*.js\"",
    "lint-scss": "./node_modules/sass-lint/bin/sass-lint.js -c .scss-lint.yml -v -q",
    "fix-js": "npm run lint-js -- --fix",
    "fix-scss": "./node_modules/sass-lint-auto-fix/dist/index.js"
  },
  "pre-commit": {
    "run": [
      "lint",
      "build",
      "add-build"
    ]
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.18.9",
    "@eluvio/elv-client-js": "^4.0.0",
    "buffer": "^5.2.1",
    "chart.js": "^2.9.4",
    "dashjs": "^3.1.2",
    "elv-components-js": "git+https://github.com/eluv-io/elv-components-js.git#645165da87ec42845a908a9ce0817db202523866",
    "hls-fix": "git@github.com:elv-peter/hls.js.git#prebuilt/bugfix/live-init-cache",
    "hls.js": "^1.2.9",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.3",
    "mux-embed": "^2.7.1",
    "pretty-bytes": "^5.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-chartjs-2": "^2.9.0",
    "react-datetime-picker": "^2.7.1",
    "react-dom": "^16.8.6",
    "urijs": "^1.19.11",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.14.5",
    "@babel/plugin-transform-regenerator": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/runtime": "^7.14.6",
    "autoprefixer": "^9.5.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-preset-mobx": "^2.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "fast-sass-loader": "^2.0.1",
    "file-loader": "^2.0.0",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "raw-loader": "^0.5.1",
    "react-truncate": "^2.4.0",
    "regenerator-runtime": "^0.12.1",
    "sass": "^1.54.0",
    "sass-lint": "^1.12.1",
    "sass-lint-auto-fix": "^0.21.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.22.1",
    "svg-inline-loader": "^0.8.0",
    "svg-inline-react": "^3.1.0",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "babel": {
    "comments": true,
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "mobx"
    ],
    "plugins": [
      "syntax-async-functions",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-jsx",
      "@babel/plugin-syntax-object-rest-spread",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-react-jsx",
      "@babel/plugin-transform-regenerator"
    ]
  }
}
